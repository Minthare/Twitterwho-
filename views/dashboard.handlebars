{{#if posts.length}}
<section>
  <h2 class="header-page">Your Posts</h2>
    {{#each posts as |post|}}
      {{> dashboard-postinfo post}}
    {{/each}}

</section>
{{/if}}


<button type="submit" class="btn"><a class="link-button" href="/dashboard/newpost">+ New Post</a></button>

<script src="/js/addnewblog.js"></script>